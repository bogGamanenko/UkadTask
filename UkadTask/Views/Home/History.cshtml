@model List<TestedUrl>

@{
    ViewBag.Title = "History";
}

@if (Model.Count > 0)
{
    <table class="table table-striped">
        <thead>
            <tr>
                <th>URL</th>
                <th>Date</th>
                <th>Request time</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Url</td>
                    <td>@item.Date</td>
                    <td>@item.RequestTimeMax</td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <h4>No history entries found</h4>
}

