@model List<TestedUrl>

<h3>Tested Results</h3>
<table class="table table-striped">
    <thead>
        <tr>     
            <th>Number</th> 
            <th>URL</th>
            <th>Minimum request time</th>
            <th>Maximum request time</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count; i++)
        {
            <tr>
                <td>@(i + 1)</td>
                <td>@Model[i].Url</td>
                <td>@Model[i].RequestTimeMin.ToString()</td>
                <td>@Model[i].RequestTimeMax.ToString()</td>
            </tr>
        }               
    </tbody>
</table>